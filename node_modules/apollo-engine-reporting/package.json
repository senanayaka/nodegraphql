  },
  "license": "MIT",
  "repository": "https://github.com/apollographql/apollo-engine-reporting",
  "author": "Apollo <community@apollographql.com>",
  "engines": {
    "node": ">=6.0"
  },
  "dependencies": {
    "apollo-engine-reporting-protobuf": "0.0.1",
    "apollo-server-env": "2.0.2",
    "async-retry": "^1.2.1",
    "graphql-extensions": "0.1.2",
    "lodash": "^4.17.10"
  },
  "gitHead": "f7b5d63d038384409fd4d664a3bcce04c721bc6d"
